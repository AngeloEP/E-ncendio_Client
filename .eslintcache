                   scaleLabel: {\n                        display: true,\n                        labelString: 'Y text'\n                    },\n                    type: 'linear',\n                    display: true,\n                    position: 'left',\n                    id: 'y-axis-1',\n                },\n                {\n                    scaleLabel: {\n                        display: true,\n                        labelString: 'Y2 text'\n                    },\n                    type: 'linear',\n                    display: true,\n                    position: 'right',\n                    id: 'y-axis-2',\n                    gridLines: {\n                      drawOnArea: false,\n                    },\n                }\n            ],\n            xAxes: [\n                {\n                    scaleLabel: {\n                        display: true,\n                        labelString: 'X text'\n                    }\n                }\n            ]\n        }\n    }\n\n    return (\n        \n        <div className={classes.divContent} >            \n            <Grid container direction=\"row\" justify=\"center\" alignItems=\"center\" spacing={0}>\n                <Grid item xs={10} className=\"mt-5\" style={{ backgroundColor: 'red' }}>\n                    <h3> xs=10 </h3>\n                </Grid>\n                \n                {/* Gráficas */}\n                {/* El Barras */}\n                <Grid item xs={5} className=\"mt-5 mr-5\" style={{ width: '100%', height: '100%' }} >\n                    <Bar data={dataBar} options={opcionesBar} />\n                </Grid>\n\n                <Grid item xs={5} className=\"mt-5\" style={{ width: '100%', height: '100%' }} >\n                    <Doughnut data={dataDoughnut} />\n                </Grid>\n\n                <Grid item xs={5} className=\"mt-5 mr-5\" style={{ width: '100%', height: '100%' }} >\n                    <Line data={dataLine} options={optionsLine} />\n                </Grid>\n                \n                <Grid item xs={5} className=\"mt-5\" >\n                    <Paper >xs=12</Paper>\n                </Grid>\n                {/*          */}\n\n                <Grid item xs={10} className=\"mt-5\" >\n                    <h2> Contáctanos </h2>\n\n                    <Typography variant=\"subtitle1\" gutterBottom>\n                        Si tienes alguna observación sobre algún problema que se te presentó en la Aplicación, no dudes\n                        en decirnos, ¡recibirás recompensas!, contáctanos aquí:\n                    </Typography>\n                </Grid>\n                <Grid container item xs={11}  className=\"mt-4  \" >\n                    <Grid item xs={6} >\n                        <Image src={incendio} />\n                    </Grid>\n\n                    <Grid item xs={6} className={classes.formContact} >    \n                        <FormContact />\n                    </Grid>\n                    \n                </Grid>\n                \n            </Grid>\n        </div>\n        \n    );\n}\n \nexport default Home;","/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/profile/Profile.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/rank/Rank.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/games/Games.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/about/About.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/template/footer/FooterPage.js",["190"],"import React from 'react';\nimport Container from '@material-ui/core/Container';\nimport Typography from '@material-ui/core/Typography';\nimport Link from '@material-ui/core/Link';\nimport CssBaseline from '@material-ui/core/CssBaseline';\nimport {useStyles} from './footerStyles';\n\nconst FooterPage = ({  }) => {\n    const classes = useStyles();\n\n    return (\n        <div className={classes.root}>\n            <CssBaseline />\n            \n                <footer className={classes.footer}>\n                    <Container maxWidth=\"xs\">\n                    <Typography variant=\"body1\">My sticky footer can be found here.</Typography>\n                    \n                    <Typography variant=\"body2\" color=\"textSecondary\">\n                    {'Copyright © '}\n                    <Link color=\"inherit\" href=\"https://material-ui.com/\">\n                        E-ncendio\n                    </Link>{' '}\n                    {new Date().getFullYear()}\n                    {'.'}\n                    </Typography>\n\n                    </Container>\n                </footer>\n        </div>\n    )\n}\n \nexport default FooterPage;","/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/games/associateWords/AssociateWord.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/games/tagImages/TagImage.js",["191","192"],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/help/Help.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/games/fourImagesOneWord/FourImagesOneWord.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/types/index.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/games/gamesStyles.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/template/footer/footerStyles.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/home/homeStyles.js",[],"/home/angelo/Escritorio/e-ncendio_test/cliente/src/components/home/formContact/FormContact.js",["193","194","195","196"],"import { Button, CardActions, TextareaAutosize, TextField } from '@material-ui/core';\nimport React, { useState } from 'react';\nimport ToggleButton from '@material-ui/lab/ToggleButton';\nimport Box from '@material-ui/core/Box';\n\nconst styles = {\n    \n    root: {\n        justifyContent: 'center'\n    }\n};\n\nconst FormContact = () => {\n\n    const [ usuario, guardarUsuario ] = useState({\n        username: '',\n        